package com.ict.edu;

public class Ex06 {
	public static void main(String[] args){
		// 다중 for문 : for문안에 다른 for문이 존재하는 것
		for (int i = 1; i < 4; i++) {
			for (int j = 1; j < 6; j++) {
				System.out.println("i =" + i +", j = "+j);
			}
		}

		
		// 구구단
		for (int i = 2; i < 10; i++) {
			System.out.println(i + "단");
			for (int j = 1; j < 10; j++) {
				System.out.println(i + "*" + j + "=" + (i*j));
			}
		}
		
		
		// 1
	
		for (int i = 2; i < 10; i++) {
			for (int j = 1; j < 10; j++) {
				System.out.println(i + "*" + j + "=" + i*j);			
			}

		}
		
		// 2
		for (int i = 0; i < args.length; i++) {
			for (int j = 0; j < args.length; j++) {
				
			}
		}
	
		// 3
		
		
		
		}	
	} 

	
